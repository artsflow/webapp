    "heic2any": "^0.0.3",
    "humanize-duration": "^3.25.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "next": "10.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.0.5",
    "next-react-svg": "^1.1.2",
    "nprogress": "^0.2.0",
    "popmotion": "^9.3.1",
